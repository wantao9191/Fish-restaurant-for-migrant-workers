/* components/talk-item/index.wxss */
.talk-wrap {
  text-align: left;
  margin-bottom: 24rpx;
  background: #F9F9F9;
  
  padding: 24rpx;
  
}
.talk-wrap.has-border {
  border: 1px solid #ddd;
  border-radius: 10rpx;
}
.talk-wrap.no-border{
  border-bottom: 1px solid #ddd;
}
.talk-header {
  display: flex;
  justify-content: space-between;
  align-items: flex-start;
}

.icons .iconfont {
  font-size: 20rpx;
  margin-left: 14rpx;
  color: #666;
  vertical-align: middle;

}

.talk-header-wrap {
  display: flex;
}

.talk-header .name {
  font-size: 20rpx;
  color: #666;
  margin-bottom: 6rpx;
}

.talk-header .time {
  font-size: 18rpx;

  color: #999;
}

.talk-header image {
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
}

.talk-header-aside {
  margin-left: 14rpx;
}

.talk-main {
  font-size: 24rpx;
  font-family: Verdana, Geneva, Tahoma, sans-serif;
}

.talk-covers {
  display: flex;
  margin-top: 24rpx;
}

.talk-covers .cover-item.alt {
  background-image: url(data:image/jpg;base64,);
}

.talk-main-title {
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: break-word;
  white-space: normal !important;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.talk-covers .cover-item {
  flex: 1;
  max-width: 33%;
  height: 140rpx;
  background-size: cover;
  background-position: center center;
  background-repeat: no-repeat;
  margin-right: 14rpx;
  border-radius: 10rpx;
}